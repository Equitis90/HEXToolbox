<% if @scroll %>
// Append new data
$( "<%=j render( :partial => 'game_objects', :object => @game_objects ) %>" )
    .appendTo( $( ".infinite-table table" ) );
<% else %>
// Replace table
$( ".infinite-table table" ).html("<%=j render( :partial => 'game_objects', :object => @game_objects ) %>" );
<% end %>

// Update pagination link
<% if @game_objects.last_page? %>
$( '.pagination' ).html( "That's all, folks!" );
<% else %>
$( '.pagination' )
    .html( "<%=j link_to_next_page( @game_objects, 'Next Page', :remote => true, :params => { :scroll => true, :set_selector => @set_selector, :card_name => @card_name } ) %>" );
<% end %>